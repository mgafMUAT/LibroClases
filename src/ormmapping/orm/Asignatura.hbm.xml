<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Asignatura" table="asignatura" lazy="false">
		<id name="id" column="id_pk" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<many-to-one name="curso_id_fk" cascade="lock" column="curso_id_fk" class="orm.Curso" not-null="false" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="profesorid_pk" cascade="lock" column="profesorid_pk" class="orm.Profesor" not-null="false" lazy="proxy" access="field">
		</many-to-one>
		<property name="nombre" column="nombre" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Actividad" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="asignatura_id_fk" not-null="true"/>
			<one-to-many class="orm.Actividad"/>
		</set>
	</class>
</hibernate-mapping>
