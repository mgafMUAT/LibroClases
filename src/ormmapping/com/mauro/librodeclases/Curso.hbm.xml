<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mauro.librodeclases.Curso" table="Curso" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="colegio" cascade="lock" column="ColegioID" class="com.mauro.librodeclases.Colegio" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="nivel" column="Nivel" type="byte" length="3" not-null="true" lazy="false"/>
		<property name="letra" column="Letra" type="character" length="1" not-null="true" lazy="false"/>
		<many-to-one name="ramo" column="CursoID" class="com.mauro.librodeclases.Ramo" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<set name="ORM_Alumnos" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="CursoID" not-null="true"/>
			<one-to-many class="com.mauro.librodeclases.Alumno"/>
		</set>
	</class>
</hibernate-mapping>
