<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mauro.librodeclases.Ramo" table="Ramo" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="profesor" column="ProfesorID" class="com.mauro.librodeclases.Profesor" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="nombre" column="Nombre" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Planificacion" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="RamoID" not-null="true"/>
			<one-to-many class="com.mauro.librodeclases.Actividad"/>
		</set>
	</class>
</hibernate-mapping>
