<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mauro.librodeclases.Profesor" table="Profesor" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="colegio" cascade="lock" column="ColegioID" class="com.mauro.librodeclases.Colegio" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="nombre" column="Nombre" type="string" length="255" not-null="false" lazy="false"/>
		<property name="rut" column="Rut" type="string" length="255" not-null="false" lazy="false"/>
		<one-to-one name="ramo" class="com.mauro.librodeclases.Ramo" cascade="save-update,lock" property-ref="profesor" lazy="proxy" access="field"/>
	</class>
</hibernate-mapping>
