<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mauro.librodeclases.Alumno" table="Alumno" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="registro" column="AsistenciaID" class="com.mauro.librodeclases.Asistencia" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="apoderado" cascade="lock" column="ApoderadoID" class="com.mauro.librodeclases.Apoderado" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="curso" cascade="lock" column="CursoID" class="com.mauro.librodeclases.Curso" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="ingreso" column="Ingreso" type="byte" length="3" not-null="true" lazy="false"/>
		<property name="matricula" column="Matricula" type="string" length="255" not-null="false" lazy="false"/>
		<many-to-one name="nota" column="NotaID" class="com.mauro.librodeclases.Nota" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<set name="ORM_Anotaciones" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="AlumnoID" not-null="true"/>
			<one-to-many class="com.mauro.librodeclases.Anotacion"/>
		</set>
	</class>
</hibernate-mapping>
