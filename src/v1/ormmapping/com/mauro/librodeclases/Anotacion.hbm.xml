<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mauro.librodeclases.Anotacion" table="Anotacion" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="observacion" column="Observacion" type="string" length="255" not-null="false" lazy="false"/>
		<property name="esPositiva" column="EsPositiva" type="boolean" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
